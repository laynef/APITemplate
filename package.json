{
    "name": "api-template",
    "version": "1.0.0",
    "description": "",
    "main": "app.js",
    "scripts": {
        "start": "node app.js",
        "test": "node tests/index.js",
        "lint": "eslint -c .eslintrc.js . --fix",
        "validate": "npm ls",
        "security": "snyk test",
        "linter-catch": "git add .",
        "migrate": "sequelize db:migrate",
        "unmigrate": "sequelize db:migrate:undo:all",
        "seed": "sequelize db:seed:all",
        "refreshdb": "npm run unmigrate && npm run migrate && npm run seed"
    },
    "author": "Layne Faler",
    "license": "ISC",
    "devDependencies": {
        "chai": "^4.1.2",
        "eslint": "^4.18.0",
        "eslint-config-standard": "^11.0.0-beta.0",
        "eslint-plugin-import": "^2.8.0",
        "eslint-plugin-node": "^6.0.0",
        "eslint-plugin-promise": "^3.6.0",
        "eslint-plugin-standard": "^3.0.1",
        "mocha": "^5.0.1",
        "nodemon": "^1.15.0",
        "precommit-hook": "^3.0.0",
        "sequelize-cli": "^3.2.0",
        "snyk": "^1.69.7",
        "swagger": "^0.7.5"
    },
    "dependencies": {
        "bcrypt-nodejs": "0.0.3",
        "body-parser": "^1.18.2",
        "compression": "^1.7.1",
        "dotenv": "^5.0.0",
        "elasticsearch": "^14.1.0",
        "express": "^4.16.2",
        "express-rate-limit": "^2.11.0",
        "express-session": "^1.15.6",
        "helmet": "^3.11.0",
        "jsonwebtoken": "^8.1.1",
        "lodash": "^4.17.5",
        "morgan": "^1.9.0",
        "multer": "^1.3.0",
        "nodemailer": "^4.4.2",
        "pg": "^6.4.2",
        "pg-native": "^2.2.0",
        "pug": "^2.0.0-rc.4",
        "spdy": "^3.4.7",
        "stripe": "^5.4.0"
    },
    "pre-commit": [
        "lint",
        "validate",
        "test",
        "linter-catch"
    ]
}
